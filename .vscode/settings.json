  // ========== TODO TREE ==========
  "todo-tree.general.tags": [
    "BUG:",
    "HACK:",
    "FIXME:",
    "TODO:",
    "XXX",
    "[ ]",
    "[x]",
    "NOTE:",
    "NEED PERF:"
  ],
  "todo-tree.highlights.defaultHighlight": {
    "icon": "check",
    "type": "tag",
    "background": "#D0104C",
    "opacity": 100,
    "foreground": "#FCFAF2",
    "borderRadius": "2px"
  },
  "todo-tree.highlights.customHighlight": {
    "BUG:": {
      "icon": "bug",
      "background": "#D0104C",
      "iconColour": "#ff0"
    },
    "HACK:": {
      "icon": "tools",
      "background": "#947A6D",
      "iconColour": "#947A6D"
    },
    "FIXME:": {
      "icon": "flame",
      "background": "#f00",
      "iconColour": "#f00"
    },
    "TODO:": {
      "icon": "check",
      "background": "#E03C8A",
      "iconColour": "#E03C8A"
    },
    "NOTE:": {
      "icon": "file",
      "background": "#006284",
      "iconColour": "#006284"
    },
    "NEED PERF:": {
      "icon": "rocket",
      "background": "#6A4C9C",
      "iconColour": "#6A4C9C"
    },
    "XXX": {
      "icon": "x"
    },
    "[ ]": {
      "icon": "issue-draft"
    },
    "[x]": {
      "icon": "issue-closed"
    }
  },
  "todo-tree.highlights.foregroundColourScheme": [],
  "todo-tree.highlights.backgroundColourScheme": [],

  "todohighlight.defaultStyle": {
    "color": "red",
    "cursor": "pointer",
    "border": "1px solid red",
    "backgroundColor": "rgba(0,0,0,.2)",
    "borderRadius": "2px",
    "isWholeLine": false
  },
  "todohighlight.keywords": [
    "DEBUG:",
    "REVIEW:",
    {
      "text": "NOTE:",
      "color": "#006284",
      "border": "1px solid #006284",
      "overviewRulerColor": "grey"
    },
    {
      "text": "FIXME:",
      "color": "red",
      "border": "1px solid red",
      "backgroundColor": "none",
      "overviewRulerColor": "grey"
    },
    {
      "text": "HACK:",
      "color": "#947A6D",
      "border": "1px solid #947A6D",
      "backgroundColor": "none",
      "isWholeLine": false
    },
    {
      "text": "TODO:",
      "color": "#E03C8A",
      "border": "1px solid #E03C8A",
      "borderRadius": "2px",
      "backgroundColor": "rgba(0,0,0,.2)"
    },
    {
      "text": "NEED PERF:",
      "color": "#6A4C9C",
      "border": "1px solid #6A4C9C",
      "borderRadius": "2px",
      "backgroundColor": "rgba(0,0,0,.2)"
    }
  ],
  "markdown-preview-enhanced.enablePreviewZenMode": false,
  "markdown-preview-enhanced.previewTheme": "medium.css",
  "markdown-preview-enhanced.revealjsTheme": "white.css",
